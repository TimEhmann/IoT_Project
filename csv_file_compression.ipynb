   "source": [
    "import pandas as pd\n",
    "import glob\n",
    "\n",
    "# turn csv files into parquet\n",
    "csv_file_path = 'data/*.csv'\n",
    "files_to_convert = glob.glob(csv_file_path)\n",
    "for file in files_to_convert:\n",
    "    df = pd.read_csv(file)\n",
    "    df.to_parquet(file.replace('.csv', '.parquet'), index=False)\n"
   ]
  }
